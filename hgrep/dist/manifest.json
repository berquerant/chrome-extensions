{
  "author": "berquerant",
  "manifest_version": 3,
  "name": "hgrep",
  "description": "Grep history of visited pages",
  "homepage_url": "https://github.com/berquerant/chrome-extensions/hgrep",
  "version": "0.6.0",
  "permissions": ["history", "tabs", "storage", "clipboardWrite"],
  "action": {
    "default_title": "hgrep",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
